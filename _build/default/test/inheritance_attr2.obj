class circle {
  attribute int z;

  method int get_z() {
    return this.z + 97;
  }
}

class point extends circle {
  attribute int x;
  attribute int y;
  method void constructor(int x, int y) {
    this.x = x;
    this.y = y;
    this.z = x + y;
  }
  method int sum(int x) {
    return (x+this.x+this.y);
  }
}

class carre extends point {
  method void constructor(int x) {
    this.x = x;
    this.y = x;
    this.z = x;
  }
}

function void main() {
  var point p;
  var point p2;
  var int c;
  var carre carre;
  p = new point(1, 2);
  p2 = new point(5, -5);
  c = p.sum(48);
  carre = new carre(26);
  putchar(p2.get_z());
  putchar(p.get_z());
  putchar(carre.get_z());
  putchar(c);
}
